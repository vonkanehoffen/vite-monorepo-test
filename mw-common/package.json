{
  "name": "@mw/mw-common",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/mw-common.umd.cjs",
  "module": "./dist/mw-common.js",
  "exports": {
    ".": {
      "import": "./dist/mw-common.js",
      "require": "./dist/mw-common.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist-types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p build-types build-only",
    "build-watch": "run-p build-types-watch build-only-watch",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only-watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "build-types": "vue-tsc --declaration --emitDeclarationOnly",
    "build-types-watch": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8"
  }
}
